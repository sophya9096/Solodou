{
  "name": "Solodou",
  "version": "1.2.3",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean": "npx react-native-clean-project",
    "bump": "export APP_VER=`jq .version package.json` && jq \".version = $APP_VER\" app.json > tmpapp.json && mv tmpapp.json app.json",
    "postversion": "yarn bump; npx react-native-version; yarn build:apk;",
    "build:apk": "rm -rf \".builds/apk/*\"; mkdir -p .builds/apk/; cd android; ./gradlew assembleRelease; mv app/build/outputs/apk/release/app-release.apk ../.builds/apk/\"$(date '+%Y%m%d%H%M')-$npm_package_name-v$(jq -r .version ../package.json)-release.apk\";echo \"Build moved to .builds/apk\" ",
    "build:bundle": "rm -rf \".builds/bundle/*\"; mkdir -p .builds/bundle/; cd android; ./gradlew bundleRelease; mv app/build/outputs/bundle/release/app.aab ../.builds/bundle/\"$(date '+%Y%m%d%H%M')-$npm_package_name-v$(jq -r .version ../package.json)-release.aab\";echo \"Build moved to .builds/bundle\" "
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts/"
    ]
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.0",
    "@react-native-community/cli-platform-android": "^4.13.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/picker": "^1.8.0",
    "@react-native-community/push-notification-ios": "^1.8.0",
    "@react-navigation/drawer": "^5.9.0",
    "@react-navigation/native": "^5.7.3",
    "@react-navigation/stack": "^5.9.0",
    "axios": "^0.20.0",
    "react": "17.0.1",
    "react-native": "0.64.1",
    "react-native-animatable": "^1.3.3",
    "react-native-circular-action-menu": "^0.5.0",
    "react-native-collapsible": "^1.5.3",
    "react-native-country-picker-modal": "^2.0.0",
    "react-native-elements": "^2.2.1",
    "react-native-fbsdk": "^2.0.0",
    "react-native-flags": "^1.0.0",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-image-picker": "^2.3.4",
    "react-native-keyboard-aware-scroll-view": "^0.9.4",
    "react-native-modal": "^11.5.6",
    "react-native-paper": "^4.0.1",
    "react-native-permissions": "^2.2.2",
    "react-native-phone-input": "^0.2.4",
    "react-native-progress": "^4.1.2",
    "react-native-push-notification": "^7.3.1",
    "react-native-radial-context-menu": "^0.0.2",
    "react-native-radial-menu": "^1.1.0",
    "react-native-rate": "^1.2.4",
    "react-native-reanimated": "^1.13.0",
    "react-native-render-html": "^4.2.3",
    "react-native-responsive-screen": "^1.4.1",
    "react-native-safe-area-context": "^3.1.7",
    "react-native-screens": "^2.10.1",
    "react-native-sound": "^0.11.0",
    "react-native-sound-player": "^0.10.4",
    "react-native-store-rating": "^1.0.1",
    "react-native-vector-icons": "^7.0.0",
    "react-native-webview": "^10.8.3",
    "react-native-youtube-iframe": "^1.3.0",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.9",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/runtime": "^7.11.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "jest": "^26.4.2",
    "metro-react-native-babel-preset": "^0.63.0",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "postinstall": "npx patch-package"
}
